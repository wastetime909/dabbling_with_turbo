<%= turbo_frame_tag dom_id(project) do %>
  <p>
    <strong>Name:</strong>
    <%= project.name %>
  </p>

  <p>
    <strong>Active:</strong>
    <%= project.active %>
  </p>

    <%= link_to "Edit this project", edit_project_path(project), target: :_top %>

    <%= button_to "Destroy this project", project, "data-turbo-method": :delete, "data-turbo-confirm": "Are you sure?" %>

<% end %>
